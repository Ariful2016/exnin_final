            android:name="com.exnin.onlinelearning.Activities.WebViewActivity"
            android:exported="true"
            android:parentActivityName="com.exnin.onlinelearning.Activities.DashboardActivity" />
        <activity
            android:name="com.exnin.onlinelearning.Activities.SignInActivity"
            android:exported="true" />
        <activity
            android:name="com.exnin.onlinelearning.Activities.CourseDetailsActivity"
            android:exported="true"
            android:parentActivityName="com.exnin.onlinelearning.Activities.OurCourseActivity" />
        <activity
            android:name="com.exnin.onlinelearning.Activities.FlashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.exnin.onlinelearning.Activities.TeacherRegisterActivity"
