        android:supportsRtl="true"
        android:theme="@style/Theme.NetSchool24"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.exnin.onlinelearning.Activities.PaymentActivity"
            android:exported="false" />
        <activity
            android:name="com.exnin.onlinelearning.Activities.TeacherProfileActivity"
            android:exported="false"
            android:parentActivityName="com.exnin.onlinelearning.Activities.CartCheckoutActivity"/>
        <activity
            android:name="com.exnin.onlinelearning.Activities.CartCheckoutActivity"
            android:exported="false"
            android:parentActivityName="com.exnin.onlinelearning.Activities.CartActivity" />
        <activity
            android:name="com.exnin.onlinelearning.Activities.PrivacyPolicyActivity"
            android:exported="false"
            android:parentActivityName="com.exnin.onlinelearning.Activities.DashboardActivity" />
        <activity
            android:name="com.exnin.onlinelearning.Activities.CartActivity"
            android:exported="false"
            android:parentActivityName="com.exnin.onlinelearning.Activities.CourseDetailsActivity" />



        <activity
            android:name="com.exnin.onlinelearning.Activities.JoinFreeLearningActivity"
