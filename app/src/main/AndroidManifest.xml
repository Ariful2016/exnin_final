            android:parentActivityName="com.exnin.onlinelearning.Activities.DashboardActivity" />
        <activity
            android:name="com.exnin.onlinelearning.Activities.YourProfileActivity"
            android:exported="true" />
        <activity
            android:name="com.exnin.onlinelearning.Activities.AboutUsActivity"
            android:exported="true"
            android:parentActivityName="com.exnin.onlinelearning.Activities.DashboardActivity" />
        <activity android:name="com.exnin.onlinelearning.Activities.DashboardActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
